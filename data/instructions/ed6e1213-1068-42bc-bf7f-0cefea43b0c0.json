    "output": {
      "description": "データ分析結果",
      "type": "dict",
      "format": "{\"analysis_type\": str, \"target_column\": str, \"results\": dict, \"summary\": str}",
      "example": "{\"analysis_type\": \"basic_stats\", \"target_column\": \"age\", \"results\": {\"mean\": 25.5, \"median\": 24.0, \"std\": 5.2}, \"summary\": \"データの基本統計情報\"}"
    },
    "validation": {
